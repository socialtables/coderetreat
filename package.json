{
  "name": "coderetreat",
  "version": "1.0.0",
  "description": "Starter Repo for Day of CodeRetreat",
  "main": "./src/main.js",
  "scripts": {
    "start": "ts-node ./src/main.ts",
    "format": "prettier --write .",
    "test": "jest",
    "watch": "jest --watch"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "ts-jest": "26.4.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^10.17.44",
    "prettier": "^2.1.2"
  }
}
